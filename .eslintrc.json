{
    "extends": [ "eslint:recommended", "plugin:react/recommended" ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "restParams": true,
            "spread": true
        },
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": "off"
    },
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ]
}